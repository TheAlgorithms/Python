s=str(input())
t=str(input())
def isIsomorphic(s, t):
        map1 = []
        map2 = []
        for idx in s:
            map1.append(s.index(idx))
        for idx in t:
            map2.append(t.index(idx))
        if map1 == map2:
            return True
        return False
ans=isIsomorphic(s,t)
print(ans)
